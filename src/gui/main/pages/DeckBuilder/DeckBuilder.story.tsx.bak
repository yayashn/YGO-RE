import Roact from "@rbxts/roact";
import { withHookDetection } from "@rbxts/roact-hooked";
import DeckBuilder from "./DeckBuilder";

withHookDetection(Roact)

export = (target: Instance) => {    
    let tree = Roact.mount((
        <DeckBuilder />
    ),
    target, "UI")

    return () => {
        Roact.unmount(tree)
    }
}